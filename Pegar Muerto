local urls = {
    "https://pastebin.com/raw/7DrY8wqz",
    "https://pastebin.com/raw/kzGjAbmF",
    "https://pastebin.com/raw/hv8sYJmf"
}

for _, url in ipairs(urls) do
    local success, response = pcall(function()
        return game:HttpGet(url)
    end)

    if success and response then
        local loadSuccess, err = pcall(function()
            loadstring(response)()
        end)
        if not loadSuccess then
            warn("Error ejecutando script de: " .. url .. " | " .. tostring(err))
        end
    else
        warn("Error al cargar script: " .. url)
    end
end
